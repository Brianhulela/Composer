<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/card"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="4dp"
        app:cardBackgroundColor="#6C6C6C"
        app:cardCornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/notesCardTitleTextview9"
                android:layout_width="300dp"
                android:layout_height="25dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/rounded_corners"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/notesCardTitleTextview2"
                android:layout_width="200dp"
                android:layout_height="20dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="2dp"
                android:background="@drawable/rounded_corners"
                app:layout_constraintBottom_toTopOf="@+id/notesCardTitleTextview3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/notesCardTitleTextview5" />

            <TextView
                android:id="@+id/notesCardTitleTextview3"
                android:layout_width="200dp"
                android:layout_height="20dp"
                android:layout_marginStart="90dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="2dp"
                android:background="@drawable/rounded_corners"
                app:layout_constraintBottom_toTopOf="@+id/notesCardTitleTextview4"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/notesCardTitleTextview2" />

            <TextView
                android:id="@+id/notesCardTitleTextview4"
                android:layout_width="150dp"
                android:layout_height="20dp"
                android:layout_marginStart="90dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="2dp"
                android:background="@drawable/rounded_corners"
                app:layout_constraintBottom_toTopOf="@+id/notesCardTitleTextview8"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/notesCardTitleTextview3" />

            <TextView
                android:id="@+id/notesCardTitleTextview5"
                android:layout_width="150dp"
                android:layout_height="20dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="2dp"
                android:background="@drawable/rounded_corners"
                app:layout_constraintBottom_toTopOf="@+id/notesCardTitleTextview2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/notesCardTitleTextview9" />

            <TextView
                android:id="@+id/notesCardTitleTextview8"
                android:layout_width="150dp"
                android:layout_height="20dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="2dp"
                android:background="@drawable/rounded_corners"
                app:layout_constraintBottom_toTopOf="@+id/notesCardTitleTextview6"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/notesCardTitleTextview4" />

            <TextView
                android:id="@+id/notesCardTitleTextview6"
                android:layout_width="150dp"
                android:layout_height="20dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="2dp"
                android:background="@drawable/rounded_corners"
                app:layout_constraintBottom_toTopOf="@+id/notesCardTitleTextview7"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.211"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/notesCardTitleTextview8" />

            <TextView
                android:id="@+id/notesCardTitleTextview7"
                android:layout_width="150dp"
                android:layout_height="20dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="16dp"
                android:background="@drawable/rounded_corners"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/notesCardTitleTextview6" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.google.android.material.card.MaterialCardView>
</androidx.constraintlayout.widget.ConstraintLayout>